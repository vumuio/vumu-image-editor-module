    position: sticky;
    top: -1px;
    z-index: 3;
    background: #fff;

.font-centered-class
    font-size: 12px;
    color: #6B7C94;

.relative
    position:relative;

.btn-primary
    width: 167px;
    height: 45px;
    background: #0057ff;
    border-radius: 5px;
    color: #fff;
    font-size: 14px;
    font-weight: 500;
    font-family: Inter;
    border: none;
    margin: 0 auto;
    display: block;

.setBgButton,
.buttons-area .btn-primary.setBgButton
    background: #fafafa;
    color: #8492a6;
    border: 2px solid #8492a6;
    padding: 0 7px;

.pointer
    cursor:pointer;

.tui-image-editor-container.left .tui-image-editor-submenu .tui-image-editor-submenu-item li,
.tui-image-editor-container.right .tui-image-editor-submenu .tui-image-editor-submenu-item li
