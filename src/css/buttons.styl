    margin-top:20px !important;

.submenu-buttons button
    border: 2px solid #c4c4c4;
    width: 80%;
    display: block;
    margin: 0 auto 12px;
    color: #6b7c94;
    padding: 8px 5px;

li.tie-font-family-button
